{
  "name": "mikrotik-hotspot-platform",
  "version": "1.0.0",
  "description": "MikroTik Hotspot Management Platform",
  "main": "dist/main/main.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:api\" \"npm run dev:renderer\"",
    "dev:renderer": "vite --port 5173",
    "dev:api": "nodemon --watch src --ext ts --exec \"ts-node src/api/server.ts\"",
    "build": "npm run build:renderer && npm run build:api",
    "build:renderer": "vite build",
    "build:api": "tsc --project tsconfig.json",
    "start": "node dist/api/server.js",
    "test": "echo \"Tests will be added later\"",
    "lint": "echo \"Linting will be added later\"",
    "setup": "node setup.js"
  },
  "keywords": [
    "mikrotik",
    "hotspot",
    "management"
  ],
  "author": "MikroTik Hotspot Platform",
  "license": "MIT",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.20",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/node": "^18.19.0",
    "@types/react": "^18.2.25",
    "@types/react-dom": "^18.2.11",
    "@types/sqlite3": "^3.1.11",
    "@vitejs/plugin-react": "^4.1.0",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "vite": "^4.5.0"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.16",
    "@mui/material": "^5.14.17",
    "@mui/x-date-pickers": "^8.12.0",
    "axios": "^1.5.1",
    "bcryptjs": "^2.4.3",
    "chart.js": "^4.5.0",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.17.0",
    "sqlite3": "^5.1.6"
  }
}
